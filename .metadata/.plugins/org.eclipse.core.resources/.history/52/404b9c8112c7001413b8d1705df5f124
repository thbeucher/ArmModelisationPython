def fileSaving(nameFile, data):
    folder = "/home/beucher/workspace/ArmModelPython/FileProcessing/RegressionResults/"
    nameToSave = folder + nameFile
    with open(nameToSave, "w") as file:
        file.write(str(data))
        
        
